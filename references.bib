  shorttitle = {Evaluating {{Machine Learning}}--{{Based Automated Personalized Daily Step Goals Delivered Through}} a {{Mobile Phone App}}},
  author = {Zhou, Mo and Fukuoka, Yoshimi and Mintz, Yonatan and Goldberg, Ken and Kaminsky, Philip and Flowers, Elena and Aswani, Anil},
  year = {2018},
