  pmid = {28058409},
  keywords = {Intervention,Mobile Health (mHealth),Stress Management},
  file = {/Users/kendrawyant/Zotero/storage/LDXDHMAU/Sarker et al. - 2016 - Finding Significant Stress Episodes in a Discontin.pdf}
}

@article{satianiProjectedWorkforcePsychiatrists2018,
  title = {Projected {{Workforce}} of {{Psychiatrists}} in the {{United States}}: {{A Population Analysis}}},
