@incollection{campbellFirstWaveScalable2023,
  title = {First Wave of Scalable Digital Therapeutics: {{Internet-based}} Programs for Direct-to-Consumer Standalone Care for Mental Health and Addiction},
  booktitle = {Digital {{Therapeutics}} for {{Mental Health}} and {{Addiction}}},
  author = {Campbell, Aimee N. C. and Brezing, Christina A. and Shulman, Matisyahu},
  year = {2023},
  month = jan,
  pages = {19--29},
  publisher = {Academic Press},
  doi = {10.1016/B978-0-323-90045-4.00005-8},
  urldate = {2022-12-12},
  abstract = {Mental health and substance use disorders are highly prevalent and are the leading cause of global disease burden worldwide. Nevertheless, most persons with mental health or substance use disorders do not receive care. Prominent barriers to traditional care include (1) stigma, (2) lack of culturally competent treatments, (3) prohibitively high cost and lack of insurance coverage, (4) inability to attend appointments due to transportation or available appointment times, and (5) inability to find coverage and prohibitively long wait times resulting from structural problems with an inadequate number of mental health providers. With the proliferation of the internet and mobile phones, digital therapeutics (software designed to prevent, treat, or manage a medical disorder or disease) are fundamentally able to address these issues. As digital therapeutics are (1) scalable, (2) constantly accessible, (3) less stigmatizing, (4) cost-effective, and (5) culturally adaptable, digital therapeutics are poised to transform mental health and substance use care.},
  isbn = {978-0-323-90045-4},
  langid = {english},
  keywords = {Digital health,Digital therapeutics,Digital treatments,mHealth,Mobile health}
}

