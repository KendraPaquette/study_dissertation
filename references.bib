  author = {Japuntich, Sandra J. and Zehner, Mark E. and Smith, Stevens S. and Jorenby, Douglas E. and Valdez, Jos{\'e} A. and Fiore, Michael C. and Baker, Timothy B. and Gustafson, David H.},
  year = {2006},
